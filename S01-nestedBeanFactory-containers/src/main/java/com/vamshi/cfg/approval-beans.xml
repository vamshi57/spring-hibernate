<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="loan" class="com.vamshi.loan.LoanDetails">
        <property name="loanId" value="101"/>
        <property name="loanType" value="education"/>
        <property name="customerName" value="vamshi"/>
      </bean>
  
   <bean id="id" class="com.vamshi.loan.BankLoanApprover">
     <property name="details">
     <ref bean="id"/>
    </property>
   </bean>

</beans>
