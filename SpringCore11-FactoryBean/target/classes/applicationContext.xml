<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="dfb" class="com.vamshi.beans.DateFactoryBean">
	     <property name="year" value="2020"></property>
	     <property name="month" value="5"></property>
	     <property name="date" value="16"></property>
    </bean>
    <bean id="schedule" class="com.vamshi.beans.ScheduleRemainder">
       <constructor-arg ref="dfb"/>
    </bean>
</beans>